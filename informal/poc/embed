#include <embedded>

namespace std {
	/* some magical implementation */
	constexpr static const char local_data[] = { '0', '1', '2' };
	constexpr embedded embed( std::string_view path_ ) {
		constexpr const char* const data = local_data;
		return embedded(data, sizeof(local_data) / sizeof(*data), embedded_source::file);
	}
}
